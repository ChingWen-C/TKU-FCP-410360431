public class ch2_1 { //定義public類別ch2_1
    public static void main(String[] args) {//main函數，主程式開始
        int num1=2;  //宣告整數num
        int num2=3;
        float num3=4;
        System.out.println(num1+" cats are running");
        System.out.println(num2+" dogs are running" );
        System.out.println(num3+" birds are running" );
    }
}
