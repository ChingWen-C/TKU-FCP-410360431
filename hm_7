import java.util.Scanner;
public class hm_7 {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in); 
        System.out.print("半徑: ");
        double num = scn.nextDouble();
        double area = Math.PI * Math.pow(num, 2);
        System.out.println("面積: " + area);
        scn.close();
    }
}
