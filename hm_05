public class hm_05 {
    public static void main(String[] args) {
        int a = 12;
        int b = 7;
        System.out.printf("和= %d\n",(a+b));
        System.out.printf("差= %d\n",(a-b));
        System.out.printf("積= %d\n",(a*b));
        System.out.printf("商= %d\n",(a/b)); 
        System.out.printf("餘數= %d\n",(a%b));
    }
}
